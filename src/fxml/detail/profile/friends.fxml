<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.detail.profile.FriendsController">
   <center>
      <VBox spacing="5.0" BorderPane.alignment="CENTER">
         <children>
            <ComboBox fx:id="cb_pseudonym" accessibleRole="TEXT_FIELD" editable="true" prefWidth="400.0" promptText="Search Player" />
            <Button fx:id="btn_add" mnemonicParsing="false" text="Add" />
            <ListView prefHeight="200.0" prefWidth="200.0" />
            <Button fx:id="btn_remove" mnemonicParsing="false" text="Remove" />
            <Label text="* enter Pseudonym">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </Label>
            <Label fx:id="lbl_error" text="errorl" visible="false" />
         </children>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
      </VBox>
   </center>
</BorderPane>
